{
    "compilerOptions": {
        "jsx": "react",
        "target": "es2019",
        "module": "commonjs",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "strict": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "allowJs": true,
        "resolveJsonModule": true,
        "incremental": false,
        "outDir": "./dist",
        "moduleResolution": "node"
    },
    "include": [
        "."
    ],
    "exclude": [
        ".git",
        "**/node_modules/*",
        "dist",
        "pack"
    ]
}
